<% include ../../template/style.ejs %>
<% include ../../template/header.ejs %>
<% include ../../template/menu.ejs %>

<!-- Content -->

<!-- BEGIN: Content-->
<div class="app-content content">
    <div class="content-overlay"></div>
    <div class="content-wrapper">
        <div class="content-header row">
            <div class="content-header-left col-12 mb-2 mt-1">
                <div class="row breadcrumbs-top">
                    <div class="col-12">
                        <h5 class="content-header-title float-left pr-1 mb-0"><%= swj.label %></h5>
                        <div class="breadcrumb-wrapper col-12">
                            <ol class="breadcrumb p-0 mb-0">
                                <li class="breadcrumb-item">
                                    <a href="/">
                                        <i class="bx bx-home-alt"></i>
                                    </a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a href="">SWJ</a>
                                </li>
                                <li class="breadcrumb-item active">
                                    <a href="<%= swj.url %>">List <%= swj.label %></a>
                                </li>
                                <li class="breadcrumb-item active">
                                    <a href="">Add</a>
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="content-body">

            <!-- Basic Horizontal form layout section start -->
            <section id="basic-horizontal-layouts">
                <div class="row match-height">
                    <div class="col-md-6 col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Form <%= swj.label %> Baru</h4>
                            </div>
                            <div class="card-content">
                                <div class="card-body">
                                    <form id="add_data_form">
                                        <div class=" form-body">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <label>Nama</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="nama" class="form-control" name="nama"
                                                        required placeholder="Nama <%= swj.label %>"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Price</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="price" class="form-control" name="price"
                                                        required placeholder="<%= Math.floor(Math.random() * 100) %>000"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Latitude</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="latitude" class="form-control"
                                                        name="latitude" required placeholder="-6.8810365"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Longitude</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="longitude" class="form-control"
                                                        name="longitude" required placeholder="107.5662972"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Deskripsi</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <textarea type="text" id="description" class="form-control"
                                                        name="description"
                                                        placeholder="Deskripsikan tentang <%= swj.label %>" required
                                                        data-validation-required-message="This field is required"></textarea>
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Address</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <textarea type="text" id="address" class="form-control"
                                                        name="address"
                                                        placeholder="Kp. Sawah Jl. Letnan Sukarna Desa Benteng" required
                                                        data-validation-required-message="This field is required"></textarea>
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Image</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="image" class="form-control" name="image"
                                                        required placeholder="Upload"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-md-4">
                                                    <label>Featured</label>
                                                </div>
                                                <div class="col-md-8 form-group">
                                                    <input type="text" id="fitur" class="form-control" name="fitur"
                                                        required placeholder="WC, Parkir, Dll"
                                                        data-validation-required-message="This field is required">
                                                </div>
                                                <div class="col-sm-12 d-flex justify-content-end">
                                                    <button type="submit"
                                                        class="btn btn-primary mr-1 mb-1 form-group">Add</button>
                                                    <button type="reset"
                                                        class="btn btn-light-secondary mr-1 mb-1">Reset</button>
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- // Basic Horizontal form layout section end -->

        </div>
    </div>
</div>
<!-- END: Content-->

<!-- End Content -->

<% include ../../template/footer.ejs %>

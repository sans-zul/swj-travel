<% include ../template/head.ejs %>
<% include ../template/style.ejs %>
<% include ../template/header.ejs %>
<% include ../template/menu.ejs %>

<!-- BEGIN: Content-->
<div class="app-content content">
    <div class="content-overlay"></div>
    <div class="content-wrapper">
        <div class="content-header row">
        </div>
        <div class="content-body">
            <!-- Knowledge base Jumbotron start -->
            <section class="kb-search">
                <div class="row">
                    <div class="col-12">
                        <div class="card bg-transparent shadow-none">
                            <div class="card-content">
                                <div class="card-body text-center">
                                    <h1 class=" mb-2 kb-title">Hi <%= data_admin.name %>, Welcome to the Dashboard</h1>
                                    <p class=" mb-3">
                                        Bring a smile to West Java with Tron.
                                    </p>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="kb-content">
                <div class="row kb-search-content-info mx-1 mx-md-2 mx-lg-5">
                    <div class="col-12">
                        <div class="row match-height">
                            <div class="col-md-2"></div>
                            <div class="col-md-4 col-sm-6 kb-search-content">
                                <div class="card kb-hover-2">
                                    <div class="card-content">
                                        <div class="card-body text-center">
                                            <a href="">
                                                <div class=" mb-1">
                                                    <i class="livicon-evo"
                                                        data-options="name: map.svg; size: 50px; strokeColorAlt: #FDAC41; strokeColor: #5A8DEE; style: lines-alt; eventOn: .kb-hover-2;"></i>
                                                </div>
                                                <h5><%= data_dashboard.wisata %> Tempat</h5>
                                                <p class=" text-muted">Wisata</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-6 kb-search-content">
                                <div class="card kb-hover-4">
                                    <div class="card-content">
                                        <div class="card-body text-center">
                                            <a href="">
                                                <div class=" mb-1">
                                                    <i class="livicon-evo"
                                                        data-options="name: home.svg; size: 50px; strokeColorAlt: #FDAC41; strokeColor: #5A8DEE; style: lines-alt; eventOn: .kb-hover-4;"></i>
                                                </div>
                                                <h5><%= data_dashboard.hotel %> Buah</h5>
                                                <p class=" text-muted">Hotel</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2"></div>
                            <div class="col-md-2"></div>
                            <div class="col-md-4 col-sm-6 kb-search-content">
                                <div class="card kb-hover-5">
                                    <div class="card-content">
                                        <div class="card-body text-center">
                                            <a href="">
                                                <div class=" mb-1">
                                                    <i class="livicon-evo"
                                                        data-options="name: shoppingcart.svg; size: 50px; strokeColorAlt: #FDAC41; strokeColor: #5A8DEE; style: lines-alt; eventOn: .kb-hover-5;"></i>
                                                </div>
                                                <h5><%= data_dashboard.rumah_makan %> Rumah</h5>
                                                <p class=" text-muted">Rumah Makan</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-6 kb-search-content">
                                <div class="card kb-hover-6">
                                    <div class="card-content">
                                        <div class="card-body text-center">
                                            <a href="">
                                                <div class=" mb-1">
                                                    <i class="livicon-evo"
                                                        data-options="name: car.svg; size: 50px; strokeColorAlt: #FDAC41; strokeColor: #5A8DEE; style: lines-alt; eventOn: .kb-hover-6;"></i>
                                                </div>
                                                <h5><%= data_dashboard.transportasi %> Unit</h5>
                                                <p class=" text-muted">Transportasi</p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2"></div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>
<!-- END: Content-->


<% include ../template/footer.ejs %>
<% include ../template/script.ejs %>
<% include ../template/bottom.ejs %>